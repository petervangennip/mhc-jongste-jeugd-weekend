{
  "private": true,
  "scripts": {
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "node-version": "node ./scripts/switch-node-version.js",
    "createssl": "node ./scripts/create-ssl.js",
    "pre-commit": "lint-staged",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "build": "nuxt build",
    "dev": "nuxt dev --https --ssl-cert ./.ssl/localhost.pem --ssl-key ./.ssl/localhost-key.pem"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,md,json,css}": [
      "prettier --write"
    ],
    "**/*.{js,ts,vue,html}": [
      "npm run lint:fix"
    ]
  },
  "devDependencies": {
    "@chenfengyuan/vue-countdown": "^2.1.2",
    "@dargmuesli/nuxt-cookie-control": "^6.4.4",
    "@lucien144/vue3-parallaxy": "^0.1.1",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "^1.0.0",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@nuxtjs/html-validator": "^1.5.2",
    "@nuxtseo/module": "^2.0.0-beta.15",
    "@rushstack/eslint-patch": "^1.5.0",
    "@storyblok/vue": "^7.2.0",
    "@types/node": "^20.7.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.8.1",
    "nuxt-icon": "^0.5.0",
    "postcss": "^8.4.30",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "swiper": "^10.3.1",
    "tailwindcss": "^3.3.3",
    "vite-plugin-eslint": "^1.8.1",
    "vue-collapsed": "^1.2.8"
  },
  "dependencies": {
    "@storyblok/nuxt": "^5.7.1"
  }
}
